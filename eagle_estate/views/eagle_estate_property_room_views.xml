<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_eagle_property_room_form" model="ir.ui.view">
            <field name="name">eagle.estate.property.room.form</field>
            <field name="model">eagle.estate.property.room</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="type"/>
                            <field name="area"/>
                            <field name="property_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="eagle_estate_property_room_tree" model="ir.ui.view">
            <field name="name">eagle.estate.property.room.tree</field>
            <field name="model">eagle.estate.property.room</field>
            <field name="arch" type="xml">
                <list>
                    <field name="type"/>
                    <field name="area"/>
                    <field name="property_id"/>
                </list>
            </field>
        </record>

    </data>
</odoo>
